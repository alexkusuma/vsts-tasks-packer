{
  "name": "vsts-packer-tasks",
  "version": "0.0.1",
  "description": "Packer Tasks for VSTS",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:prod": "npm i && npm run build && npm prune --production && npm dedupe --production",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "upload": "npm run build:prod && tfx build tasks upload --task-path ."
  },
  "keywords": [
    "packer",
    "vsts"
  ],
  "author": "Manuel Riezebosch",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2",
    "azure-pipelines-tool-lib": "^0.13.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@types/node-fetch": "^2",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "typescript": "^4.5.2"
  }
}
