{
  "name": "vsts-packer-tasks",
  "version": "0.0.1",
  "description": "Packer Tasks for VSTS",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "build:prod": "npm i && npm run build && npm prune --production && npm dedupe",
    "pretest": "npm run build",
    "test": "mocha --recursive",
    "upload": "npm run build:prod && tfx build tasks upload --task-path ."
  },
  "keywords": [
    "packer",
    "vsts"
  ],
  "author": "Manuel Riezebosch",
  "license": "ISC",
  "dependencies": {
    "fs": "0.0.1-security",
    "azure-pipelines-task-lib": "^2.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "sinon": "^8.1.0",
    "stryker": "^0.35.1",
    "stryker-api": "^0.24.1",
    "stryker-baseline-reporter": "^1.0.3",
    "stryker-html-reporter": "^0.18.1",
    "stryker-mocha-framework": "^0.15.1",
    "stryker-mocha-runner": "^0.17.1",
    "stryker-typescript": "^0.18.1",
    "typescript": "^3.7.5"
  }
}
